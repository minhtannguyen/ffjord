   "source": [
    "%%bash\n",
    "\n",
    "### Prepare ImageNet 32x32 and 64x64 ### \n",
    "DATA_DIR=/tandatasets/small_imnet\n",
    "\n",
    "mkdir ${DATA_DIR}\n",
    "cd ${DATA_DIR}\n",
    "\n",
    "for FILENAME in train_32x32.tar valid_32x32.tar train_64x64.tar valid_64x64.tar\n",
    "do\n",
    "    curl -O http://image-net.org/small/$FILENAME\n",
    "    tar -xvf $FILENAME\n",
    "done"
   ]
